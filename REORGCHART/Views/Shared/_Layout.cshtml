    <style type="text/css">
        @@media (min-width: 768px) {
            .modal-dialog {
                width: 700px !important;
            }
        }

        @@media (min-width: 1024px) {
            .modal-dialog {
                width: 1000px !important;
            }
        }

        #loading-img {
            background: url(~/assets/images/Velocity.gif) center center no-repeat;
            height: 100%;
            z-index: 20;
            position: fixed;
            top: 50%;
            left: 50%;
            margin-left: -170px;
            margin-top: -320px;
        }

        .center-div {
            width: 390px;
            height: 80px;
            text-align: left;
            position: fixed;
            top: 50%;
            left: 50%;
            margin-left: -170px;
            margin-top: -20px;
            border: 1px solid lightgray;
            border-radius: 5px;
            font-size: 22px;
            font-weight: bold;
            background-color: white;
            padding: 10px;
        }

        .overlay {
            background: #e9e9e9;
            display: none;
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            opacity: 0.2;
        }

        body {
            font-family: "Open Sans","Helvetica Neue","Helvetica",Arial,Verdana,sans-serif;
            font-weight: normal;
            font-style: normal;
            font-size: 14px;
        }

        .radio {
            font-family: "Open Sans","Helvetica Neue","Helvetica",Arial,Verdana,sans-serif;
            font-weight: normal;
            font-style: normal;
            font-size: 14px;
            padding: 2px;
            display: inline-block;
        }

        .label {
            font-family: "Open Sans","Helvetica Neue","Helvetica",Arial,Verdana,sans-serif;
            font-weight: normal;
            font-style: normal;
            font-size: 14px;
            display: inline-block;
            color: #333;
            text-align: left !important;
        }

        .select {
            font-family: "Open Sans","Helvetica Neue","Helvetica",Arial,Verdana,sans-serif;
            font-weight: normal;
            font-style: normal;
            font-size: 11px;
        }

        .hidePreview {
            display: none;
        }

        .showPreview {
            display: inline-block;
        }

        .back[class^="fa-"] {
            border: 2px solid;
            border-radius: 50% 50% 50% 50%;
            margin: 0 1px;
            padding: 7px 9px;
            text-shadow: 0 0 0;
            transition: all .25s ease;
        }

        /* The side navigation menu */
        .mainsidenav {
            height: 100%; /* 100% Full-height */
            width: 0; /* 0 width - change this with JavaScript */
            position: fixed; /* Stay in place */
            z-index: 5000; /* Stay on top */
            top: 0; /* Stay at the top */
            left: 0;
            background-color: #343135; /* Black*/
            overflow-x: hidden; /* Disable horizontal scroll */
            overflow-y: hidden; /* Disable horizontal scroll */
            padding-top: 35px; /* Place content 60px from the top */
            transition: 0.5s; /* 0.5 second transition effect to slide in the sidenav */
        }

            /* The navigation menu links */
            .mainsidenav a {
                /*padding: 8px 8px 8px 32px;*/
                text-decoration: none;
                font-size: 12px;
                color: #cccccc;
                display: block;
                transition: 0.3s;
            }

                /* When you mouse over the navigation links, change their color */
                .mainsidenav a:hover {
                    color: #f5f5f5;
                }

            /* Position and style the close button (top right corner) */
            .mainsidenav .closebtn {
                position: absolute;
                top: 0;
                right: 2px;
                font-size: 36px;
                margin-left: 50px;
            }

        .mainsidenav {
            color: white;
        }


        /* Slider Starts*/
        .slider {
            -webkit-appearance: none;
            width: 100%;
            height: 25px;
            background: #d3d3d3;
            outline: none;
            opacity: 0.7;
            -webkit-transition: .2s;
            transition: opacity .2s;
        }

            .slider:hover {
                opacity: 1;
            }

            .slider::-webkit-slider-thumb {
                -webkit-appearance: none;
                appearance: none;
                width: 25px;
                height: 25px;
                background: #4CAF50;
                cursor: pointer;
            }

            .slider::-moz-range-thumb {
                width: 25px;
                height: 25px;
                background: #4CAF50;
                cursor: pointer;
            }

        .btn-primary.toggle-bttn {
            color: #000000;
            background-color: #f5f5f5;
            border-color: #cccccc;
        }

            .btn-primary.toggle-bttn.select,
            .btn-primary.toggle-bttn:hover {
                color: white;
                background-color: #337ab7;
                border-color: #2e6da4;
            }

        .active-class {
            color: #fff;
            background-color: #286090;
            border-color: #204d74;
        }

        .footer {
            position: fixed;
            bottom: 0;
            width: 100%;
            height: 40px;
            line-height: 40px;
            background-color: #343135;
            color: #f5f5f5;
            /*text-align: center;*/
            padding: 0px 15px;
            font-size: 13px;
            z-index: 9;
            left: 0px;
            right: 0px;
            width: 100%;
        }

            .footer ul {
                padding: 0px;
                margin: 0px;
                text-align: center;
            }

                .footer ul li {
                    display: inline-block;
                }

                    .footer ul li a {
                        color: #dddddd;
                        text-decoration: none;
                        font-size: 13px;
                        padding: 0px 10px;
                    }

                    .footer ul li:first-child a {
                        border-right: 1px solid #cccccc;
                    }

                    .footer ul li a:hover {
                        color: white;
                    }

                .footer ul.social-links {
                    text-align: right;
                }

                    .footer ul.social-links li a {
                        color: #4267B2;
                        padding: 0px 10px;
                        font-size: 16px;
                        border: none;
                    }

        #myDiagramDiv {
            margin-bottom: 70px;
        }

        .navbar {
            min-height: 42px;
        }

        .collapse {
            padding-top: 3px;
        }

        .grid {
            margin: 0 auto;
        }

        .fieldset {
            border: 1px solid #ddd;
            margin-top: 1em;
            width: 97%;
        }

            .fieldset h4 {
                font-size: 12px;
                text-align: left;
                margin-left: 10px;
            }

                .fieldset h4 span {
                    display: inline;
                    background: #fff;
                    position: relative;
                    top: -2.3em;
                }

        .center {
            position: absolute;
            margin: auto;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            width: 350px;
            max-height: 450px;
            background-color: #ffffff;
            border-radius: 3px;
            border: 3px solid lightblue;
        }

        .navIndexGreen {
            border: 5px solid #337ab7;
            width: 25px;
            height: 25px;
            background-color: Green;
            text-align: center;
            vertical-align: middle;
            font-weight: bold;
            font-family: Arial;
            color: white;
        }

        .navIndexWhite {
            border: 5px solid #caddef;
            width: 25px;
            height: 25px;
            background-color: White;
            text-align: center;
            vertical-align: middle;
            font-weight: bold;
            font-family: Arial;
            color: black;
        }

        input, button, select, textarea {
            max-width: 100% !important;
            margin-top: 5px;
        }

        canvas {
            outline: none;
        }
    </style>


    @Html.DevExpress().GetStyleSheets(
            new StyleSheet { ExtensionSuite = ExtensionSuite.NavigationAndLayout },
            new StyleSheet { ExtensionSuite = ExtensionSuite.Editors },
            new StyleSheet { ExtensionSuite = ExtensionSuite.GridView }
        )
    @Html.DevExpress().GetScripts(
            new Script { ExtensionSuite = ExtensionSuite.NavigationAndLayout },
            new Script { ExtensionSuite = ExtensionSuite.Editors },
            new Script { ExtensionSuite = ExtensionSuite.GridView }
        )
</head>
